funcao fatorial(inteiro numero) -> inteiro {
    inteiro retorno;
    se(numero > 1){
        retorno = numero * fatorial(numero - 1);
    }
    senao {
        retorno = 1;
    }

    retorne retorno;
}

principal() {
    inteiro numero;
    imprima("Fatorial de qual numero? ");
    leia(numero);

    imprima(fatorial(numero));
}